/** 访问非静态内部类的构造器 */
public class demo42 {
	public static void main(String[] args) {
		demo42 demo = new demo42();
		demo.test();
	}
	
	public void test() {
		
	}
}

class OuterClass {
	class NStaticInnerClass {
		public NStaticInnerClass() {
			System.out.println(Class.class.getSimpleName());
		}
	}
}
