import java.io.File;

public class demo73 {
	public static void main(String[] args) {
		demo73 demo = new demo73();
		switch (0) {
		case 0:
			demo.test1();
			break;
		default:
			break;
		}
	}

	/* 测试 */
	public void test1() {
		/* 文件检测相关方法 */
		File helloFile1 = new File("hello_world.cpp");
		File helloFile2 = new File("hello_world_2.cpp");
		File helloFile3 = new File("demo");
		File helloFile4 = new File("test.exe");
		
		File[] helloFiles = new File[]{
			helloFile1, helloFile2,
			helloFile3, helloFile4}
		/* 文件对象是否存在 */
		System.out.println("[Exists]" + helloFile1.exists()
				+ "/" + helloFile2.exists()
				+ "/" + helloFile3.exists()
				+ "/" + helloFile4.exists());
		/* */
	}
}
