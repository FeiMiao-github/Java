package algorithm;

import java.util.Arrays;

/* 快排算法 */
public class Demo1 {
	public static void main(String[] args) {
		int[] array = {4, 3 ,4, 6, 7,1 ,4, 3, 2};
		new Demo1().quickSort(array);
		
		var Strings = Arrays.toString(array);
		System.out.println("[array]" + Strings);
	}
	
	public void quickSort(int[] args) {
		for (int i = 1; i < args.length; i++) {
			int j = i - 1;
			int temp = args[i];
			while (j >= 0 && args[i] <= args[j]) {
			}
		}
	}
}
