/* 快排算法 */
public class Demo1 {
	public static void main(String[] args) {
		int[] array = {1, 3 ,4, 6, 7,1 ,4, 3, 2};
		new Demo1().quickSort(array);
	}
	
	public void quickSort(int[] args) {
		for (int i = 1; i < args.length; i++) {
			int j = i - 1;
			while (j-- >= 0 && args[i] < args[j]) {
				int temp = args[j];
				args[i] = args[j];
				args[j] = temp;
			}
		}
	}
}
