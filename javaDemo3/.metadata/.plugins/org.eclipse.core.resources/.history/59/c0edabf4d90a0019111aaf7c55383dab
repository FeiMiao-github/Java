package demo;

import java.util.Random;

/* 线程安全ThreadLocalRandom和Random */
public class demo66 {
	public static void main(String[] args) {
		new demo66().test();
	}
	
	public void test() {
		Random random = new Random();
		System.out.println("boolean >>>" + random.nextBoolean());
		System.out.println("Double >>>" + random.nextDouble());
	} 
}
