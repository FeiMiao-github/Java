public class demo65 {

	public static void main(String[] args) {
		new demo65().test();
	}
	
	public void test() {
		new MathTest() {
			
			@Override
			public void mathTest() {
				// 弧度转换为角度， 角度转换为弧度
				System.out.println("Math.toDegrees(1.56) --- "+ Math.toDegrees(1.56));
			}
		}.mathTest();
	}
}

interface MathTest {
	public void mathTest();
}